<p id="notice"><%= notice %></p>

<h1>Pixels</h1>

<table>
  <thead>
    <tr>
      <th>Ip</th>
      <th>track name</th>
      <th>city</th>
      <th>state</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pixels.each do |pixel| %>
      <tr>
        <td><%= pixel.ip_address %></td>
        <td><%= pixel.track.name %></td>
        <td><%= pixel.city %></td>
        <td><%= pixel.state %></td>
        <td><%= link_to 'Show', pixel %></td>
        <td><%= link_to 'Destroy', pixel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
